    "@mui/material": "^7.3.6",
    "@prisma/client": "^6.19.1",
    "adminjs": "^7.8.17",
    "csv-parser": "^3.2.0",
    "express": "^5.2.1",
    "next": "16.1.0",
    "openai": "^6.16.0",
    "pdf-parse": "^1.1.4",
